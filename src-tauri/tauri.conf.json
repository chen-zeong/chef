{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Chef",
  "version": "0.1.0",
  "identifier": "com.czeong.chef",
  "build": {
    "beforeDevCommand": "pnpm run app:dev",
    "beforeBuildCommand": "pnpm run app:build",
    "devUrl": "http://localhost:5188",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "label": "main",
        "title": "",
        "width": 1220,
        "height": 780,
        "titleBarStyle": "Overlay",
        "center": true,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "ocr_models"
    ]
  }
}
